    describe("RequestBuilder getConnectionInfo Tests", function() {&#13;
&#13;
        it("Should return a Connection &amp; Credentials object", function() {&#13;
            var builder = new x_neyu_rest_utils.RequestBuilder();&#13;
            var info = builder.getConnectionInfo("1d32c0b52b02a2508a69f0dcce91bfa0");&#13;
&#13;
            expect(info).toBeDefined();&#13;
        });&#13;
&#13;
        it("Should return null if the credentialID is invalid", function() {&#13;
            var builder = new x_neyu_rest_utils.RequestBuilder();&#13;
            var info = builder.getConnectionInfo("dslfjaiejfksdjfakwef");&#13;
&#13;
            expect(info).toBe(null);&#13;
        });&#13;
&#13;
        it("Should return null if the credentialID is null", function() {&#13;
            var builder = new x_neyu_rest_utils.RequestBuilder();&#13;
            var info = builder.getConnectionInfo();&#13;
&#13;
            expect(info).toBe(null);&#13;
